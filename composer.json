{
    "name":"am/geshi-bundle",
    "description":"Symfony2 Bundle For The Generic Syntax Highlighting (GeSHi) Package",
    "autoload":{
        "psr-0":{
            "AM\\Geshi":"src/AM/"
        }
    },
    "repositories":[
        {
            "name":"am/geshi",
            "url":"git://github.com/createvibe/GeSHi.git",
            "type":"git"
        }
    ],
    "require":{
        "php":">=5.3.3",
        "symfony/symfony":">=2.1",
        "am/geshi":"*"
    },
    "scripts":{},
    "config":{
        "bin-dir":"bin"
    },
    "minimum-stability":"dev",
    "extra":{
        "symfony-app-dir":"app",
        "symfony-web-dir":"web"
    }
}
